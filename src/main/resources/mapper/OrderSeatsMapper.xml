<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chen.cloudshadow.mapper.OrderSeatsMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.chen.cloudshadow.pojo.OrderSeats">
        <id column="order_seat_id" property="orderSeatId" />
        <result column="order_id" property="orderId" />
        <result column="seat_id" property="seatId" />
        <result column="created_at" property="createdAt" />
        <result column="updated_at" property="updatedAt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        order_seat_id, order_id, seat_id, created_at, updated_at
    </sql>

</mapper>
